{
  "sections": {
    "main": {
      "type": "article",
      "settings": {
        "show-featured": true,
        "heading-position": "left",
        "show-date": false,
        "show-author": false
      }
    },
    "navigation": {
      "type": "article-navigation",
      "settings": {
        "show-tags": true,
        "facebook-enabled": true,
        "x-enabled": true,
        "pinterest-enabled": true,
        "email-enabled": true,
        "url-enabled": true
      }
    },
    "recents": {
      "type": "article-recents",
      "settings": {
        "enable": true,
        "aspect-ratio": "landscape"
      }
    },
    "comments": {
      "type": "article-comments",
      "settings": {
        "show-comments": true,
        "disqus-enabled": false,
        "disqus-shortname": ""
      }
    }
  },
  "order": [
    "main",
    "navigation",
    "recents",
    "comments"
  ]
}
